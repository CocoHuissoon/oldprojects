package main;

import java.util.Arrays;
import java.util.Collection;

import model.Sort;
import model.Student;
import util.Shuffler;

public class SortingStudentsLauncher {

	public static void main(String[] args) {
		Shuffler sh=new Shuffler();
		Sort sort=new Sort();
		Student s= new Student();
		Student [] q=s.generateStudents(1000);
		Shuffler.shuffleArray(q);
		System.out.println(Arrays.toString(q));
		sort.selectionSort(q);
		System.out.println(Arrays.toString(q));
		
//		to do: write code to test the Student class and the sort methods in the Sort class

	}

}

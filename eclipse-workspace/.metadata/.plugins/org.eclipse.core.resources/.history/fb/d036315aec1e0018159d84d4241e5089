class Frog{
	String name;
	int age;
	
	public void setName(String newName) {
		name = newName;
	}
	
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
}
		
	

public class Setters17 {
	public static void main(String[] args) {
	
		Frog frog1 = new Frog();
		//frog1.name = "Fredje";
		//frog1.age = 12;
		frog1.setName("Henkie!!!!");
		
		
		
	}

}

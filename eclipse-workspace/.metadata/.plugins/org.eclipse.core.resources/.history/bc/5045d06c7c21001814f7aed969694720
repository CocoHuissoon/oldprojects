class Frog {
	private int id;
	private String name;
	
	public Frog (int id, String name) {
		this.id=id;
		this.name=name;
	}
	public String toString() {
		return id + ": "+ name;
	}
}
	public class ToString21 {
	public static void main (String[]args){
		Frog frog1 = new Frog(12,"Bertie");
		System.out.println(frog1);
		System.out.println(frog2);

		System.out.println();
		
		
		
		
	}

}

package nl.hva.miw.oop2.cohort12.tentamen;

public class Launcher {

	
		public static void main(String[] args) throws OngeldigeMatrixPositieException {
			Matrix q = new Matrix(5);
			Matrix d = new Matrix(5);
			d.addElement(4, 4, 4);
			q.addElement(4, 4, 4);
			System.out.println(d.compareTo(q));
			System.out.println(q.toString());
			
		}
	}


